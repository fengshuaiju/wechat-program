<!--start.wxml-->
<view class="container" wx:if="{{sales}}">
  <view class="tab-image">
    <image bindtap="tapBanner" data-id="{{sales.businessId}}" class="bgImages" src="{{sales.picUrl}}" />
    <view class="gohome" bindtap="home">跳过广告 {{second}}</view>
  </view>
  <view bindtap="home" class="logo">
    <view class="title">{{sales.title}}</view>
    <view class="subtitle">{{sales.remark}}</view>
  </view>
</view>